{
	"extends": ["airbnb-typescript-prettier"],
	"ignorePatterns": ["*.js"],
	"rules": {
		"padding-line-between-statements": "error",
		"comma-dangle": 0,
		"prettier/prettier": "error",
		"no-unused-vars": "error",
		"prefer-template": "warn",
		"import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
	}
}
